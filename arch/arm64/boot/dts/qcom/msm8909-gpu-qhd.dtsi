&msm_gpu {
	/delete-property/qcom,gpu-speed-config;
	/delete-node/qcom,gpu-speed-config@1;

	/* Power levels */
	qcom,gpu-pwrlevels {

		qcom,gpu-pwrlevel@0 {
			reg = <0>;
			qcom,gpu-freq = <307200000>;
			qcom,bus-freq = <3>;
		};

		qcom,gpu-pwrlevel@1 {
			reg = <1>;
			qcom,gpu-freq = <200000000>;
			qcom,bus-freq = <1>;
		};

		qcom,gpu-pwrlevel@2 {
			reg = <2>;
			qcom,gpu-freq = <19200000>;
			qcom,bus-freq = <0>;
		};
	};
};
